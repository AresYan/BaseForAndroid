<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="@dimen/d890"
        android:layout_height="wrap_content"
        android:background="@drawable/common_white_solid_bg"
        android:orientation="vertical">

        <TextView
            android:id="@+id/common_dialog_alert_TextView_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingTop="@dimen/d30"
            android:paddingBottom="@dimen/d30"
            android:textColor="@color/gray_333333"
            android:textSize="@dimen/sp50" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/d1"
            android:background="@color/gray_333333" />

        <FrameLayout
            android:id="@+id/common_dialog_alert_FrameLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/d1"
            android:background="@color/gray_333333" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/d158"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/common_dialog_alert_TextView_left"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:textColor="@color/black_02"
                android:textSize="@dimen/sp50" />

            <View
                android:id="@+id/common_dialog_alert_View_center"
                android:layout_width="@dimen/d1"
                android:layout_height="match_parent"
                android:background="@color/gray_333333" />

            <TextView
                android:id="@+id/common_dialog_alert_TextView_right"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:textColor="@color/black_02"
                android:textSize="@dimen/sp50" />
        </LinearLayout>
    </LinearLayout>
</FrameLayout>